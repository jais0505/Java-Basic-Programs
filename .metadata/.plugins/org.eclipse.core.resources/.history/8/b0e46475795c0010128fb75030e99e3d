
public class B extends A{

	int a=20;
	void display() {
		
		System.out.println("It's B");
		
		//super.display();
		
		super.a=10;
		
		int c=super.a+a;
		
		System.out.println(c);
	}
	
	void BaseDisplay() {
		
		super.display();
	}
	
	public static void main(String[] args) {
		
		B obj=new B();
		
		obj.BaseDisplay();
		obj.display();
		
		
		
	}
}
